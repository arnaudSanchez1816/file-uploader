{
  "name": "file-uploader",
  "version": "1.0.0",
  "description": "https://www.theodinproject.com/lessons/nodejs-file-uploader",
  "bugs": {
    "url": "https://github.com/arnaudSanchez1816/file-uploader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaudSanchez1816/file-uploader.git"
  },
  "license": "ISC",
  "author": "Arnaud Sanchez",
  "type": "commonjs",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run clean && npm run bundle && npm run tailwind",
    "start": "node ./src/index.js",
    "start:watch": "npm run start -- --watch", 
    "dev": "run-p *:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "bundle": "rollup --config --bundleConfigAsCjs",
    "bundle:watch": "rollup --watch --config --bundleConfigAsCjs",
    "tailwind": "npx @tailwindcss/cli -i ./src/stylesheets/style.css -o ./public/index.css",
    "tailwind:watch": "npm run tailwind -- --watch",
    "deploy": "npx prisma migrate deploy",
    "migrate": "npx prisma migrate dev --name $npm_config_name",
    "generate": "npx prisma generate --sql",
    "clean": "rm -r ./public/scripts && rm ./public/index.css"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@supabase/supabase-js": "^2.55.0",
    "bcryptjs": "^3.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "file-type": "^16.5.4",
    "filesize": "^11.0.2",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "humps": "^2.0.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@shoelace-style/shoelace": "^2.20.1",
    "@tailwindcss/cli": "^4.1.11",
    "daisyui": "^5.0.50",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-ejs": "^1.0.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prisma": "^6.13.0",
    "rollup": "^4.46.2",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4.1.11"
  }
}
